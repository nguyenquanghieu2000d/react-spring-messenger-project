<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
         http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd">


<!--    Car;MPG;Cylinders;Displacement;Horsepower;Weight;Acceleration;Model;Origin-->
    <changeSet id="addTableBatchCsv" author="thibaut">
        <createTable tableName="cars">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="car" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="mpg" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="cylinders" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="displacement" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="horsepower" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="weight" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="acceleration" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="model" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="origin" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>